<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="cleanup_outdated_log_cron" forcecreate='True' model="ir.cron">
        <field name="name">Cleaning up outdated logs</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="model_id" ref="model_kw_http_request_log"/>
        <field name="state">code</field>
        <field name="code">model.cron_delete_outdated_logs()</field>
    </record>
</odoo>
